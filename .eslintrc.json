{
  "env": {
      "browser": false,
      "es6": true,
      "jest/globals": true
  },
  "extends": "airbnb-base",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "no-return-await": 1,
      "array-callback-return": 1
  },
  "plugins": ["jest"]
}
